{
  "name": "@ngrok/gen-x",
  "version": "0.0.0",
  "description": "Generate package.json#exports from src",
  "author": "ngrok",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrok-oss/gen-x"
  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "scripts": {
    "compile": "echo 'No build step yet...' && exit 0",
    "fmt:check": "prettier --check .",
    "fmt": "prettier --write .",
    "prepublishOnly": "pnpm run compile",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json"
  },
  "dependencies": {
    "tinyglobby": "0.2.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@types/node": "20.14.15",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  }
}
