{
  "name": "@ngrok/gen-x",
  "version": "0.1.0",
  "description": "Generate package.json#exports from src",
  "author": "ngrok",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrok-oss/gen-x"
  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": "^20.0.0"
  },
  "keywords": [],
  "scripts": {
    "bootstrap": "tsx ./scripts/code-gen-meta.ts",
    "compile": "tsc --project tsconfig.compile.json",
    "fmt:check": "prettier --check .",
    "fmt": "prettier --write .",
    "precompile": "rm -rf dist && pnpm run bootstrap",
    "prepublishOnly": "pnpm run compile",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "bin": "./dist/cli.js",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@commander-js/extra-typings": "12.1.0",
    "commander": "12.1.0",
    "tinyglobby": "0.2.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@types/node": "20.16.6",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  }
}
